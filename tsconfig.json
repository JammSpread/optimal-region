{
  "compilerOptions": {

    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./out",
    "rootDir": "./src",
    "removeComments": true,

    "strict": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
